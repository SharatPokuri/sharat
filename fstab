/dev/mapper/cl-root     /                       xfs     defaults        0 0
UUID=eba0b564-763b-41e7-b1b0-ba11b9163be3 /boot                   xfs     defaults        0 0
/dev/mapper/cl-swap     swap                    swap    defaults        0 0
